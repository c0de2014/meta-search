[HTTP headers]
In = FALSE
Out = TRUE
Key = ".Prox: (Out) 01 [# WICHTIG! #] Prox: Ausnahmen, lokale URLs und Variablen verarbeiten [MB]"
URL = "($OHDR(User-Agent:\1)$SET(HdrUAgent=\1)|)((local.ptron/|subspace.ptron/&$LST(URLHub))|)($OHDR(Referer:\1)$SET(HdrReferer=\1)|)($LST(Ausnahmeregeln)|)"

In = FALSE
Out = TRUE
Key = ".Prox: (Out) 02 [# WICHTIG! #] Prox: Listenschreiber [MB] v.1.1.0"
URL = "listwriter.local/([^/]++)\1$SET(Liste=\1)/\?\2/&$SET(Daten=\2)$SET(temp1=$WESC($UESC($GET(Daten))))$LST(ListWriter-Lists)$LOG(CAdded to BlockList $GET(Liste): "$GET(Daten)")$RDIR(http://local.ptron/killed.gif)"

In = FALSE
Out = FALSE
Key = ".URL: (Out) 03 Externen Proxy für bestimmte URLs deaktivieren [MB] v.1.1.0"
URL = "$LST(NoExternalProxyOn)$USEPROXY(false)"

In = FALSE
Out = FALSE
Key = ".URL: (Out) 05 SSL-Manager [MB] v.1.1.0"
URL = "$URL(https://)$ASK(SSLManagerAllow, SSLManagerDeny, SSL-Verbindung nach "\h" erlauben?\n\nURL: \u, \h)"
Replace = "\k"

In = FALSE
Out = FALSE
Key = ".URL: (Out) 10 Verbindungen verhindern, auf Basis der Filterlisten [MB] v.6.0.1"
URL = "(($LST(AdHubHeader)&(^$OHDR(Referer:http(s|)://$LST(AllowBannersOn)*)|$LST(AllowBannersOn)))|($LST(AdURLs))\9$SET(URLKillSubstr=\9)$SET(URLKillList=AdURLs))&($URL(http://)(^$KEYCHK(u))$LOG(WBlocked $DTM(c): \u)((*.(gif|png|jpg|jpeg)|$TST(AdCType=gif))$RDIR(http://local.ptron/killed.gif)|(*.js(\?|(^?))|$LST(JSURLs)|$TST(AdCType=js))$RDIR(http://local.ptron/killed.js)|*.css$RDIR(http://local.ptron/killed.css)|$SET(URLKillMatched=\u)$RDIR(http://local.ptron/killed-pro.html))|$URL(https://)(^$KEYCHK(u))$LOG(WBlocked (SSL) $DTM(c): \u))"
Match = "$URL(https://)"
Replace = "\k"

In = FALSE
Out = TRUE
Key = "Accept-Encoding: (Out) Datenkompression: Nur unterstützte Verfahren erlauben [MB]"
Match = "*"
Replace = "gzip, deflate"

In = FALSE
Out = FALSE
Key = "Cache-Control: (In) Browsercache nicht beeinflussen lassen [SL]"
Match = "*"

In = FALSE
Out = FALSE
Key = "Connection: (In+Out) Verbindungen nicht wiederverwenden [SL]"
Replace = "close"

In = FALSE
Out = TRUE
Key = "Connection: (Out) Proxy vor überempfindlichen Blogspamsperren verstecken [MB]"
Match = "\1$OHDR(TE:*)"
Replace = "\1, TE"

In = FALSE
Out = FALSE
Key = "Content-Encoding: (In) Fehlerhafte Angaben entfernen [MB] v.0.0.2"
Match = "(^gzip|x-gzip|deflate)?"

In = TRUE
Out = FALSE
Key = "Content-Type: (In) 00 [# WICHTIG! #] Prox: Verarbeitung lokaler Dateien steuern [MB] v.1.0.4"
URL = "local.ptron/*.(jpg$SET(temp1=image/jpeg)|png$SET(temp1=image/png)|js$SET(temp1=text/javascript; charset=iso-8859-1)$FILTER(false)|css$SET(temp1=text/css; charset=iso-8859-1)$FILTER(false))"
Match = "*"
Replace = "$GET(temp1)"

In = TRUE
Out = FALSE
Key = "Content-Type: (In) 01 [# WICHTIG! #] Prox: Wenn bekanntes externes Skript --> Korrigieren [MB] v.2.1.1"
URL = "$LST(JSURLs)"
Match = "((^text/javascript|application/x-javascript|application/json)*)\1"
Replace = "text/javascript$LOG(BInfo $DTM(c): Known script: \1 --> text/javascript)"

In = TRUE
Out = FALSE
Key = "Content-Type: (In) 02 Falsche Angaben regelbasiert korrigieren [MB] v.1.0.4"
Match = "$RESP((200|301|302)*)(text/plain|unknown|text/x-js|application/javascript|text/script|text/x-javascript|(^?))\1$URL([^/]+/[^=?]++.([a-z0-9]+{2,8}(^?)&&$LST(CorrectContentTypes)))"
Replace = "\0$TST(CTKill=true)\k"

In = TRUE
Out = FALSE
Key = "Content-Type: (In) 03 [# WICHTIG! #] Prox: Filterung nach Content-Type steuern [MB] 1.0.1"
Match = "(^(application/x-javascript|text/javascript)$SET(CType=js)|(text/html|multipart/x-mixed-replace)$SET(CType=html)|application/xhtml\+xml$SET(CType=xhtml)|text/css$SET(CType=css)|text/vbscript$SET(CType=vbs)|image/gif$SET(CType=gif)|application/json(request|)$SET(CType=json)|text/plain$IHDR(Server: Proxomitron)$RESP(200 Local File))\1$FILTER(false)"
Replace = "\1"

In = FALSE
Out = FALSE
Key = "Content-Type: (In) 30 Bestimmte GIF-Animationen nicht einfrieren [MB]"
URL = "$LST(AllowAnimGIFs)"
Match = "(image/gif)\1$FILTER(false)"
Replace = "\1"

In = FALSE
Out = FALSE
Key = "Content-Type: (In) 31 CSS-Daten nicht filtern (Opera-Bug) [MB] v.1.0.2"
URL = "$TST(HdrUAgent=*opera*)(^$TST(ScfModify=*hdr.filterCss*))"
Match = "(text/css*)\1$FILTER(false)"
Replace = "\1$LOG(BInfo $DTM(c):CSS-Daten nicht filtern (Opera-Bug))"

In = FALSE
Out = FALSE
Key = "Cookie: (Out) 13 Tinyurl.com,Qurl.net: Vorschau aktivieren [MB] v.1.0.1"
URL = "tinyurl.com/$SET(temp1=preview=1)|forwarding.tinyurl.com/$SET(temp1=preview=1)|qurl.net/$SET(temp1=redirect_mode=Manual)"
Replace = "$GET(temp1)$LOG(VHMatch $DTM(c): Cookie: (Out) Tinyurl.com,Qurl.net: Vorschau aktivieren [MB])"


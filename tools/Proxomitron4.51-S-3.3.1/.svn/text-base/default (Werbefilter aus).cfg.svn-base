Name = "Prox: HTML: Start-Filterskript einfügen v.5.3.2"
Active = TRUE
URL = "$TYPE(htm)(^local.ptron/)(^$TST(Mtx03=1))$SET(Mtx03=1)(^$OHDR(X-Requested-With: XMLHttpRequest)|$OHDR(X-MicrosoftAjax: *))$SET(Dep04=1)$TST($DTM(tc)=(?)\1(?)\2(?)\3(?+{0,2})\4$SET(EnvConId=\2\4\1\3)*)"
Limit = 2000
Match = "("
        "(<head(\s[^>]+>| >)"
        "( <meta http-equiv=$AV(X-UA-Compatible)[^>]+>| $NEST(<title,</title>))+{0,2}"
        ")\1 "
        "|"
        "(^(^< (frameset|body|style|script|table|a\s|object|iframe|noscript|textarea|embed|applet|noframes|div|p(^[a-z]))*>))"
        ")"
        "(^$TST(SnCType=*))"
Replace = "\1\r\n\r\n"
          ""
          "$SET(\2=$TST(CfgRepVerb=*InfoTags=1*)<meta name="Proxomitron-Debug" url="\u"></meta>\r\n)"
          "$SET(\3=$TST(ScfJSAdID=*), ConfJSAdID:"$GET(ScfJSAdID)")"
          ""
          "\2"
          "<!-- <Proxomitron filter="Prox: Start-Filterskript einfuegen"> -->\r\n"
          ""
          "<script type="text/javascript" id="ProxHS_$GET(EnvConId)">\r\n"
          "(function(){"
          "if (!window.ProxLCO&&location.protocol!="file:") {\r\n"
          "ProxLCO = {Status:{HSRel:"20100221",HSVer:"5.3.2",HSVar:"HTML"}, ConfSC:"$GET(InsHeadStrSC)"\3, FS:{}}; "
          "ProxLCO.Prox_newElm = function(tag){return document.createElement(tag);}; "
          "var obSS1 = ProxLCO.Prox_newElm("script"); obSS1.type = "text/javascript"; obSS1.src = "http://local.ptron/ProxJS-"+ProxLCO.Status.HSRel+".js"; "
          "var obSS2 = ProxLCO.Prox_newElm("link"); obSS2.type = "text/css"; obSS2.rel = "stylesheet"; obSS2.href = "http://local.ptron/ProxCSS-"+ProxLCO.Status.HSRel+".css"; "
          "var dhHS = document.getElementById("ProxHS_$GET(EnvConId)"); dhHS.parentNode.appendChild(obSS1); dhHS.parentNode.appendChild(obSS2);\r\n"
          "}"
          "})();\r\n"
          "</script>\r\n"
          ""
          ""
          "$SET(ScfInsHeadStrJS=$TST(ScfInsHeadStrJS=*)<script type="text/javascript">$GET(ScfInsHeadStrJS)</script>\r\n)$GET(ScfInsHeadStrJS)"
          "<style type="text/css">.ProxJSElm{display:none;}$SET(InsHeadStrCSS=$TST(InsHeadStrCSS=*) GET(InsHeadStrCSS) )$GET(InsHeadStrCSS)</style>\r\n"
          ""
          "<!-- </Proxomitron> -->\r\n\r\n"
          ""
          "$SET(StatusHeadCode=eingefuegt)$STOP()"


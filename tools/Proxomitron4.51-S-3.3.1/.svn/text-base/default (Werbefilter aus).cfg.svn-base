Name = "Prox: (X)HTML: Aktuelle Position in Seitenstruktur ermitteln v.1.4.2"
Active = TRUE
Multi = TRUE
URL = "($TYPE(htm)|$TST(CType=xhtml))$SET(Dep01=1)$SET(PosInScript=0)$SET(PosInStyle=0)$SET(PosInLink=0)$SET(PosInTxtAr=0)$SET(PosInObj=0)$SET(PosInComm=0)"
Limit = 1000
Match = "<("
        "t[dh]$SET(PosInTableGapV=0)|"
        "/t[dh]$SET(PosInTableGapV=1)|"
        "(a\s)$TST(PosInScript=0)$SET(PosInLink=1)|"
        "/a >$SET(PosInLink=0)|"
        "tr(\s|>)$SET(PosInTableGapH=0)$SET(PosInTableGapV=1)|"
        "/tr(\s|>)$SET(PosInTableGapH=1)|"

Match = "</(body>|html>)(^$TST(PosInScript=1))$TST(StatusHeadCode=eingefuegt)(^ </noframes>)"
        "( <noscript>| <plaintext>| <object[^>]++>| <textarea[^>]++>| <layer[^>]++>| <style[^>]++>| <applet[^>]++>)+"

Name = "Prox: XHTML: End-Filterskript einfügen v.3.2.1"
Active = TRUE
URL = "$TST(CType=xhtml)"
Limit = 32
Match = "(</body>)\1"
        "$TST(StatusHeadCode=eingefuegt)"
        "$STOP()"
Replace = "\r\n\r\n"
          "$SET(InsEndStrTTTags=$TST(InsEndStrTTTags=*)<script type="text/javascript" id="ProxFS_$GET(EnvConId)">\r\n//<![CDATA[\r\n(function(){ if (window.ProxLCO) { ProxLCO.stTTipTags="$GET(InsEndStrTTTags)"; }})();\r\n//]]>\r\n</script>\r\n)$GET(InsEndStrTTTags)"
          "$SET(ScfInsEndStrJS=$TST(ScfInsEndStrJS=*)<script type="text/javascript">\r\n//<![CDATA[\r\n$GET(ScfInsEndStrJS)\r\n//]]>\r\n</script>\r\n)$GET(ScfInsEndStrJS)"
          "\1"

Name = "Prox: HTML: End-Filterskript einfügen v.3.2.1"
Active = TRUE
URL = "$TYPE(htm)"
Limit = 32
Match = "<end>"
Replace = "$TST(StatusHeadCode=eingefuegt)"
          "\r\n\r\n"
          ""
          "$SET(InsEndStrTTTags=$TST(InsEndStrTTTags=*)<script type="text/javascript" id="ProxFS_$GET(EnvConId)">\r\n(function(){ if(window.ProxLCO){ProxLCO.stTTipTags="$GET(InsEndStrTTTags)"; }})();\r\n</script>\r\n)$GET(InsEndStrTTTags)"
          "$SET(ScfInsEndStrJS=$TST(ScfInsEndStrJS=*)<script type="text/javascript">\r\n$GET(ScfInsEndStrJS)\r\n</script>\r\n)$GET(ScfInsEndStrJS)"
          ""
          "</body>\r\n"
          "</html>\r\n"

Name = "Prox: Schreibe Details in "Vorgang abgebrochen"-Seiten v.1.1.2"
Active = TRUE
Multi = TRUE
URL = "local.ptron/killed-pro.html"
Limit = 100
Match = "<ProxReplace matched URL>*</ProxReplace matched URL>$TST(URLKillMatched=*)$TST(URLKillMatched=\#(<$SET(\#=$lt;)|>$SET(\#=$gt;)|\&$SET(\#=&amp;))+\#)$SET(\1=\@)|"
        "<ProxReplace substring found in list>*</ProxReplace substring found in list>$TST(URLKillSubstr=*)$SET(\1=$GET(URLKillSubstr))|"
        "<ProxReplace name of list>*</ProxReplace name of list>$TST(URLKillList=*)$SET(\1=$GET(URLKillList))|"
        "<ProxReplace comment from list>*</ProxReplace comment from list>$TST(URLKillComment=*)$SET(\1=$GET(URLKillComment))|"
        "<ProxReplace Style>($TST(URLKillStyle=*)$SET(\1=$GET(URLKillStyle))|$SET(\1=std))"
Replace = "\1"


[Proxies]
 OpenLog = TRUE


